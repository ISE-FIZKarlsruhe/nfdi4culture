Prefix(:=<https://nfdi4culture.de/ontology/cto-edit.owl#>)
Prefix(dc:=<http://purl.org/dc/elements/1.1/>)
Prefix(mo:=<http://purl.org/ontology/mo/>)
Prefix(n4c:=<https://nfdi4culture.de/id/>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)
Prefix(schema:=<http://schema.org/>)
Prefix(dcterms:=<http://purl.org/dc/terms/>)
Prefix(nfdicore:=<https://nfdi.fiz-karlsruhe.de/ontology/>)


Ontology(<https://nfdi4culture.de/ontology/cto-edit.owl>
Import(<https://nfdi4culture.de/ontology/cto/imports/nfdicore_import.owl>)
Import(<https://nfdi4culture.de/ontology/cto/imports/schema_import.owl>)
Annotation(dcterms:description "The CTO ontology models cultural heritage research data and builds upon the NFDIcore ontology.")
Annotation(dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/>)
Annotation(dcterms:title "NFDI4Culture Ontology (CTO)")

Declaration(Class(<https://nfdi4culture.de/ontology/CTO_0001001>))
Declaration(AnnotationProperty(dcterms:description))
Declaration(AnnotationProperty(dcterms:license))
Declaration(AnnotationProperty(dcterms:title))
############################
#   Annotation Properties
############################

# Annotation Property: dcterms:description (description)

AnnotationAssertion(rdfs:label dcterms:description "description")

# Annotation Property: dcterms:license (license)

AnnotationAssertion(rdfs:label dcterms:license "license")

# Annotation Property: dcterms:title (title)

AnnotationAssertion(rdfs:label dcterms:title "title")



############################
#   Classes
############################

# Class: <https://nfdi4culture.de/ontology/CTO_0001001> (DataFeedElement)

AnnotationAssertion(rdfs:comment <https://nfdi4culture.de/ontology/CTO_0001001> "A CTO DataFeedElement is an NFDI Resource that refers to an individual data point or entry within a larger data set."@en)
AnnotationAssertion(rdfs:comment <https://nfdi4culture.de/ontology/CTO_0001001> "In CTO, the DataFeedElement serves to signify a membership within a particular resource or datafeed. Thus, entities of various typesâ€”such as of types nfdicore:Person, nfdicore:Place, cto:Item, or nfdicore:Event, etc. may acquire the additional designation of DatafeedElement if they are included in e.g. a dataset and are described within the graph."@en)
AnnotationAssertion(rdfs:label <https://nfdi4culture.de/ontology/CTO_0001001> "DataFeedElement"@de)
AnnotationAssertion(rdfs:label <https://nfdi4culture.de/ontology/CTO_0001001> "DataFeedElement"@en)
SubClassOf(<https://nfdi4culture.de/ontology/CTO_0001001> nfdicore:NFDI_0000001)

# Class: <https://schema.org/ImageObject> (ImageObject)

AnnotationAssertion(rdfs:comment <https://schema.org/ImageObject> "In the context of the Culture Ontology (CTO), an image object is an information content entity and connected to a DataFeedItem. It can have a license statement (string) and an image location."@en)
AnnotationAssertion(rdfs:label <https://schema.org/ImageObject> "ImageObject"@en)
SubClassOf(<https://schema.org/ImageObject> <http://purl.obolibrary.org/obo/IAO_0000030>)


AnnotationAssertion(rdfs:label <http://purl.obolibrary.org/obo/CTO_0000000> "root node"@en)
)