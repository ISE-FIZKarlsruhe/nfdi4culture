#################################################################
#    Data properties
#################################################################

###  https://nfdi.fiz-karlsruhe.de/ontology/NFDI_0001008
nfdicore:NFDI_0001008 rdf:type owl:DatatypeProperty .


###  https://nfdi4culture.de/ontology/CTO_0001000
<https://nfdi4culture.de/ontology/CTO_0001000> rdf:type owl:DatatypeProperty ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment """A general property used to associate a resource with a legal or rights-related statement, such as information about copyright, licensing, or usage terms. This property serves as a superproperty for more specific legal declarations. 
The preferred approach for legal statements is the usage of the object property  \"nfdicore: has license\". In cases, where \"nfdicore: has license\" cannot be used, a subproperty of \"cto: has legal statement\" can be utilized."""@en ;
                                               rdfs:label "has legal statement"@en .


###  https://nfdi4culture.de/ontology/CTO_0001007
<https://nfdi4culture.de/ontology/CTO_0001007> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001000> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "A natural language text description of a license (permissions, reuse conditions, or usage terms.). This applies for example to media objects connected to source items in the research data index."@en ;
                                               rdfs:label "has license statement"@en ;
                                               skos:example "The photograph is licensed under CC-BY-1.0"@en .


###  https://nfdi4culture.de/ontology/CTO_0001021
<https://nfdi4culture.de/ontology/CTO_0001021> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf nfdicore:NFDI_0001008 ;
                                               rdfs:comment "The location of a media object that is associated with a cto:source item."@en ;
                                               rdfs:label "has content url"@en ;
                                               skos:editorialNote "[TT] schema:contentURL could not be reused here, as it is not compatible with the ontology's OWL DL profile"@en ;
                                               skos:example "cto:source item <https://slod.fiz-karlsruhe.de/labw-2-2599048> has an image with the location <https://slod.fiz-karlsruhe.de/images/slod/2-2599048-4.jpg>"@en .


###  https://nfdi4culture.de/ontology/CTO_0001022
<https://nfdi4culture.de/ontology/CTO_0001022> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001000> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Links a resource to a textual or URI-based statement describing the intellectual property rights, copyright status, or legal ownership information associated with the resource."@en ;
                                               rdfs:label "has rights statement"@en ;
                                               skos:example "<http://rightsstatements.org/vocab/InC-EDU/1.0/>"@en ,
                                                            "In Copyright - Educational Use Permitted"@en ,
                                                            "This Item is protected by copyright and/or related rights. You are free to use this Item in any way that is permitted by the copyright and related rights legislation that applies to your use. In addition, no permission is required from the rights-holder(s) for educational uses. For other uses, you need to obtain permission from the rights-holder(s)."@en .


###  https://nfdi4culture.de/ontology/CTO_0001023
<https://nfdi4culture.de/ontology/CTO_0001023> rdf:type owl:DatatypeProperty ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Relates an instance of incipit to its literal representation, which includes the key signature, the incipit pattern, the key, clef value, key signature and time signature."@en ;
                                               rdfs:label "has incipit value"@en .


###  https://nfdi4culture.de/ontology/CTO_0001060
<https://nfdi4culture.de/ontology/CTO_0001060> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001023> ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Associates an incipit with the musical time signature it begins in, expressed as a string (e.g., \"4/4\", \"3/8\"). This property captures the rhythmic meter of the notated passage."@en ;
                                               rdfs:label "has time signature"@en ;
                                               skos:example "\"3/4\""@en .


###  https://nfdi4culture.de/ontology/CTO_0001061
<https://nfdi4culture.de/ontology/CTO_0001061> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001023> ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Specifies the musical clef used at the beginning of the incipit (e.g., \"G\", \"F\", \"C clef\"). This property indicates the pitch reference point for interpreting the staff notation."@en ;
                                               rdfs:label "has clef"@en ;
                                               skos:example "“G-2”"@en .


###  https://nfdi4culture.de/ontology/CTO_0001062
<https://nfdi4culture.de/ontology/CTO_0001062> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001023> ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Associates an incipit with its written key signature, expressed as a string (e.g., \"2 sharps\", \"Bb, Eb\"). This reflects the accidentals present at the beginning of the staff."@en ;
                                               rdfs:label "has key signature"@en ;
                                               skos:example "“xF”"@en .


###  https://nfdi4culture.de/ontology/CTO_0001063
<https://nfdi4culture.de/ontology/CTO_0001063> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001023> ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Provides a simplified textual or symbolic representation of the melodic or rhythmic pattern of the incipit."@en ;
                                               rdfs:label "has incipit pattern"@en ;
                                               skos:example "“4'D/8.6GB4AG/8.6''EC2'A/4.''D8'BAG/4BA”"@en .


###  https://nfdi4culture.de/ontology/CTO_0001064
<https://nfdi4culture.de/ontology/CTO_0001064> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf <https://nfdi4culture.de/ontology/CTO_0001023> ;
                                               rdfs:domain <https://nfdi4culture.de/ontology/CTO_0001024> ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "Indicates the tonal key in which the incipit is set, such as \"C major\" or \"A minor\". Unlike the key signature, this represents the actual tonality of the passage, which may differ due to modality or historical notation conventions."@en ;
                                               rdfs:label "has key"@en ;
                                               skos:example "\"G major\""@en .


###  https://nfdi4culture.de/ontology/CTO_0001067
<https://nfdi4culture.de/ontology/CTO_0001067> rdf:type owl:DatatypeProperty ;
